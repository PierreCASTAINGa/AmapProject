<!DOCTYPE html>

<title>Categorie</title>

{% block body %}
    <h1>Categorie</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ categorie.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ categorie.nom }}</td>
            </tr>
            <tr>
                <th>NbCheque</th>
                <td>{{ categorie.nbCheque }}</td>
            </tr>
            <tr>
                <th>NbChequeFixe</th>
                <td>{{ categorie.nbChequeFixe ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Remarque</th>
                <td>{{ categorie.remarque }}</td>
            </tr>
            <tr>
                <th>Actif</th>
                <td>{{ categorie.actif ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>UrlImg</th>
                <td>{{ categorie.urlImg }}</td>
            </tr>
            <tr>
                <th>DescriptionProduits</th>
                <td>{{ categorie.descriptionProduits }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('categorie_index') }}">back to list</a>

    <a href="{{ path('categorie_edit', {'id': categorie.id}) }}">edit</a>

    {{ include('frontend/categorie/_delete_form.html.twig') }}
{% endblock %}
