<form method="post" action="{{ path('super_categorie_delete', {'id': super_categorie.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ super_categorie.id) }}">
    <button class="btn">Supprimer</button>
</form>
